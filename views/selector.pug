extends layout

block content
  h1= title
  - var image_names=["img1","img2","img3","img4","img5"]
  - var i=0,value
  - while(i<3)
    {
       value=Math.floor(Math.random()*5) 
       img(src=`/images/${image_names[value]}` alt=`car images ${value}` height="300px" width="300px") 
       i++
    -}     